<launch>

  <!--  map to load -->
  <arg name="map" default="lab_test2.yaml" />
  <!-- start map server -->
  <node name="map_server" pkg="map_server" type="map_server" args="$(find atrv_mini_pkg)/maps/$(arg map)"/>

  <!-- start rgb camera -->
  <node pkg="image_view" type="image_view" name="image_view" output="screen">
     <remap from="/image" to="/camera/rgb/image_raw"/>
  </node>

  <!-- start amcl -->
  <include file="$(find atrv_mini_pkg)/launch/amcl.launch" />

  <!--- start rviz
  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find atrv_mini_pkg)/rviz/navigation.rviz"/>
   -->

</launch>
